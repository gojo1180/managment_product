{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "template/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/products/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/auth/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "template/index.html"
    }
  ]
}